{
  "name": "mawebdk/loggerfactory",
  "description": "Simple LoggerFactory to support creation of PSR-3 logger objects without dependency injection.",
  "license": "BSD-2-Clause",
  "require": {
    "php": "^8.3.6",
    "psr/log": "^3.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0.2"
  },
  "autoload": {
    "psr-4": {
      "MawebDK\\LoggerFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MawebDK\\LoggerFactory\\Test\\": "test/"
    }
  }
}
